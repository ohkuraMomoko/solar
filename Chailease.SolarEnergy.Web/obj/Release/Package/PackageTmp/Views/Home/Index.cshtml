@section scripts {
<script>
        $(function () {
            // Page.Init();
            $("#bannerSlider").slick({
                dots: true,
                autoplay: true,
                arrows: true,
                autoplaySpeed: 5000
            });
        
        });

        // 參與募資 VIEW MORE 按鈕
        $(".project-item-btn").on("click", function () {
            var href = $(this).data("href");
            //GA: 圖靈/增強型電子商務-Product Click
            dataLayer.push({
                'ecommerce': {
                    'click': {
                        'products': [{
                            'id': $(this).data("ga-project-id"),
                            'name': $(this).data("name"),
                            'category': "太陽能板",
                            'price': $(this).data("price")
                        }]
                    }
                },
                'event': 'td_productClick'
            });
            window.location = href;
        });

